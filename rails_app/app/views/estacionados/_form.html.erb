    <%= form.label :placa %>
    <%= form.text_field :placa, id: :estacionado_placa %>
  </div>

  <div class="field">
    <%= form.label :entrada %>
    <%= form.datetime_select :entrada, id: :estacionado_entrada %>
  </div>

  <div class="field">
    <%= form.label :saida %>
    <%= form.datetime_select :saida, id: :estacionado_saida %>
  </div>

  <div class="field">
    <%= form.label :valor %>
    <%= form.text_field :valor, id: :estacionado_valor %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
