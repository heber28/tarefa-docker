<%= form_with(model: estacionado, local: true) do |form| %>
  <% if estacionado.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(estacionado.errors.count, "error") %> prohibited this estacionado from being saved:</h2>

      <ul>
      <% estacionado.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :tipo_veiculo %>
    <%= form.text_field :tipo_veiculo, id: :estacionado_tipo_veiculo %>
