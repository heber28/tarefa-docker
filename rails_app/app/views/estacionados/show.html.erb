<%- model_class = Estacionado -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:tipo_veiculo) %>:</strong></dt>
  <dd><%= @estacionado.tipo_veiculo %></dd>
  <dt><strong><%= model_class.human_attribute_name(:placa) %>:</strong></dt>
  <dd><%= @estacionado.placa %></dd>
  <dt><strong><%= model_class.human_attribute_name(:entrada) %>:</strong></dt>
  <dd><%= @estacionado.entrada %></dd>
  <dt><strong><%= model_class.human_attribute_name(:saida) %>:</strong></dt>
  <dd><%= @estacionado.saida %></dd>
  <dt><strong><%= model_class.human_attribute_name(:valor) %>:</strong></dt>
  <dd><%= @estacionado.valor %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              estacionados_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_estacionado_path(@estacionado), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              estacionado_path(@estacionado),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
